<customUI xmlns='http://schemas.microsoft.com/office/2009/07/customui' onLoad='OnLoad'>
  <ribbon startFromScratch='false'>
    <tabs>
      <tab id='tabPublish' label='Dans'>
        <group id='Group1' label='Publish'>
          <!-- https://bert-toolkit.com/imagemso-list.html -->
          <!-- https://docs.microsoft.com/en-us/openspecs/office_standards/ms-customui/ec42bfd0-149c-495b-895c-3bc708b8a149-->
          <button id='btnStartSharing' label='Start Sharing' imageMso='MacroPlay' size='large' onAction='StartPublishPositions'/>
          <button id='btnStopSharing' label='Stop Sharing' imageMso='MacroRecorderPause' size='large' onAction='StopPublishPositions'/>
        </group>
        <group id='Group2' label='Check'>
          <button id='btnCheckDBConnection' label='Check DB' imageMso='OutlineDemoteToBodyText' size='large' onAction='TestDbConnection'/>
          <button id='btnCheckConfigConnection' label='Check Config' imageMso='AcceptInvitation' size='large' onAction='CheckConfig'/>
        </group>
		<group id='Group3' label='Save'>
			<button id='btnSaveFile' label='Save to File' imageMso='FileSave' size='large' onAction='SaveFile'/>
		</group>
      </tab>
    </tabs>
  </ribbon>
</customUI>